{
  "name": "minelev-notifications",
  "description": "Notifications for MinElev",
  "version": "1.1.2",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "dummy": "micro-dev dummy.js -p 8000",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "MinElev",
    "notifications",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/minelev-notifications.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/minelev-notifications/issues"
  },
  "homepage": "https://github.com/telemark/minelev-notifications#readme",
  "now": {
    "alias": [
      "notifications.minelev.no"
    ],
    "type": "npm",
    "dotenv": "production.env"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.1",
    "micro-dev": "3.0.0",
    "nsp": "3.2.1",
    "nyc": "11.7.3",
    "standard": "11.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "jsonwebtoken": "8.2.1",
    "marked": "0.3.19",
    "micro": "9.3.0",
    "sendgrid": "5.2.3",
    "winston": "2.4.2",
    "winston-papertrail": "1.0.5"
  }
}
